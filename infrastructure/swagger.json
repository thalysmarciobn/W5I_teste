{
    "openapi": "3.0.0",
    "info": {
        "title": "API Locação de Carros",
        "version": "0.1"
    },
    "paths": {
        "/api/carros/lista": {
            "get": {
                "summary": "Lista todos os carros",
                "description": "Retorna uma lista de todos os carros cadastrados",
                "operationId": "ea70398880412eb8972f0488d00cda7d",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Carro"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/carros/modelos": {
            "get": {
                "summary": "Lista todos os modelos de carros",
                "description": "Retorna uma lista de modelos de todos os carros cadastrados",
                "operationId": "fb64986603c0b91264ee57b14a6218e5",
                "responses": {
                    "200": {
                        "description": "Operação Concluída",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/CarroModelo"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/categorias/lista": {
            "get": {
                "summary": "Lista todas as categorias",
                "description": "Retorna uma lista de categorias de veículos",
                "operationId": "640ee2c431e1c82a42656b61220ab1e6",
                "responses": {
                    "200": {
                        "description": "Operação Concluída",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Categoria"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/categorias/remover": {
            "delete": {
                "summary": "Remove uma categoria",
                "description": "Remove uma categoria pelo seu ID",
                "operationId": "62227e7589bb3b89a2d6b1631bcbf831",
                "parameters": [
                    {
                        "name": "id",
                        "in": "query",
                        "description": "ID da categoria a ser removida",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Operação Concluída"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Carro": {
                "title": "Carro",
                "description": "Entidade dos Carros",
                "properties": {
                    "placa": {
                        "description": "Placa do Veículo,",
                        "type": "string"
                    },
                    "modelo": {
                        "$ref": "#/components/schemas/CarroModelo"
                    },
                    "cor": {
                        "description": "Cor do Veículo,",
                        "type": "string"
                    },
                    "entrada": {
                        "description": "Data de Entrada do Veículo,",
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    },
                    "saida": {
                        "description": "Data de Saída do Veículo,",
                        "type": "string",
                        "format": "date-time",
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "CarroModelo": {
                "title": "CarroModelo",
                "description": "Entidade dos Modelos de Veículos",
                "properties": {
                    "nome": {
                        "description": "Nome do Modelo do Veículo,",
                        "type": "string"
                    },
                    "descricao": {
                        "description": "Descrição do Modelo do Veículo,",
                        "type": "string"
                    },
                    "categoria": {
                        "$ref": "#/components/schemas/Categoria"
                    }
                },
                "type": "object"
            },
            "Categoria": {
                "title": "Categoria",
                "description": "Entidade dos Categorias",
                "properties": {
                    "nome": {
                        "description": "Categoria de Veículo",
                        "type": "string"
                    },
                    "taxa": {
                        "description": "Taxa de Veículo",
                        "type": "string",
                        "format": "float"
                    }
                },
                "type": "object"
            }
        }
    }
}