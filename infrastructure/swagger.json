            },
            "CarroModelo": {
                "title": "CarroModelo",
                "description": "Entidade dos Modelos de Veículos",
                "properties": {
                    "nome": {
                        "description": "Nome do Modelo do Veículo,",
                        "type": "string"
                    },
                    "descricao": {
                        "description": "Descrição do Modelo do Veículo,",
                        "type": "string"
                    },
                    "categoria": {
                        "$ref": "#/components/schemas/Categoria"
                    }
                },
                "type": "object"
            },
            "Categoria": {
                "title": "Categoria",
                "description": "Entidade dos Categorias",
                "properties": {
                    "nome": {
                        "description": "Categoria de Veículo",
                        "type": "string"
                    }
                },
                "type": "object"
            }
        }
