{
    "openapi": "3.0.0",
    "info": {
        "title": "API Locação de Carros",
        "version": "0.1"
    },
    "paths": {
        "/api/carros/lista": {
            "get": {
                "summary": "Lista todos os carros",
                "description": "Retorna uma lista de todos os carros cadastrados",
                "operationId": "ea70398880412eb8972f0488d00cda7d",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Carro"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/carros/modelos": {
            "get": {
                "summary": "Lista todos os modelos de carros",
                "description": "Retorna uma lista de modelos de todos os carros cadastrados",
                "operationId": "fb64986603c0b91264ee57b14a6218e5",
                "responses": {
                    "200": {
                        "description": "Operação Concluída",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/CarroModelo"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/categorias": {
            "get": {
                "summary": "Lista todas as categorias",
                "description": "Retorna uma lista de categorias de veículos",
                "operationId": "131310a1db9e3c781b3f475f16fea79f",
                "responses": {
                    "200": {
                        "description": "Operação Concluída",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Categoria"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
